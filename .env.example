# RapidAPI Configuration (Required)
# Get your API key from https://rapidapi.com/letscrape-6bRBa3QguO5/api/jsearch
RAPIDAPI_KEY=your_rapidapi_key_here
RAPIDAPI_HOST=jsearch.p.rapidapi.com

# LinkedIn Job Search Configuration
SEARCH_QUERIES=software engineer,data engineer,python developer
SEARCH_LOCATION=United States
NUM_PAGES=1
RESULTS_PER_PAGE=10
SCHEDULE_INTERVAL_MINUTES=60
RUN_MODE=scheduler

# Snowflake Configuration (Optional - for data warehouse integration)
# Copy this file to .env and fill in your Snowflake credentials

# Snowflake account identifier (e.g., xy12345.us-east-1)
SNOWFLAKE_ACCOUNT=your_account_here

# Snowflake credentials
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password

# Snowflake target database and schema
SNOWFLAKE_DATABASE=CDC_DB
SNOWFLAKE_SCHEMA=PUBLIC
SNOWFLAKE_WAREHOUSE=COMPUTE_WH

# Consumer configuration
BATCH_SIZE=100
POLL_TIMEOUT=1.0
LOG_LEVEL=INFO
